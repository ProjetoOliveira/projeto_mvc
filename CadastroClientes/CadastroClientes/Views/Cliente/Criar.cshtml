@{
    ViewData["Title"] = "Insere Cliente";
}

<h1>INSERIR NOVO CLIENTE</h1>
  <div class="mb-3 row">
    <label for="staticEmail" class="col-sm-2 col-form-label">Email</label>
    <div class="col-sm-10">
        <input type="text" readonly class="form-control" id="staticEmail">

    </div>
  </div>
  <div class="mb-3 row">
    <label for="inputPassword" class="col-sm-2 col-form-label">Senha</label>
    <div class="col-sm-10">
      <input type="password" class="form-control" id="inputPassword">
    </div>
  </div>
  <a asp-controller="Cliente" asp-action="Index" role="button" class="btn btn-primary">Enviar</a>
<a asp-controller="Cliente" asp-action="Index" role="button" class="btn btn-primary">Voltar</a>
